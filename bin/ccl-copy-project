import os
import sys
if "CCL_HOME" in os.environ:
    sys.path.append(os.path.join(os.environ["CCL_HOME"], "common", "lib"))
    sys.path.append(os.path.join(os.environ["CCL_HOME"], "flow", "lib"))
import cskcommands.flow.copyproject

if __name__ == '__main__':
    cmd = cskcommands.flow.copyproject.CopyProject()
    cmd.main()

